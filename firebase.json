
{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1"
    }
  },
  "functions": [
    {
      "source": ".firebase/okidex-investment-match/functions",
      "codebase": "firebase-frameworks-okidex-investment-match",
      "runtime": "nodejs20",
      "deps": [
        "cors",
        "firebase-admin"
      ]
    }
  ],
  "firestore": {
    "rules": "firestore.rules"
  },
  "storage": {
    "rules": "storage.rules"
  }
}

